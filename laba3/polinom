import sys

def formula(x): # Решение формулы
    if x == 0:
        return 0
    return 1/int(x)*3

if __name__ == '__main__':
    # Считывание аргумента командной строки
    lines = sys.argv[1]
    lines = lines.split('--poly=')
    lines = lines[1].split(',')

    summ = 0
    for x in lines:
        summ += formula(x)
    print(lines)
    print(summ)
